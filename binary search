#Binary_search
def binary_search(arr,target):
lb=0
ub=len(arr)-1

while lb <= ub:
mid= (lb+ub) //2

if arr[mid]==target:
     print("element is found at the index",mid)
     return mid
elif arr[mid] < target:
     lb = mid + 1
else:
     ub = mid - 1
print("element is not found")
return -1
arr=[10,20,30,40,50]
target=40
binary_search(arr,target)
